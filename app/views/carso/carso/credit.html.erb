              <div class="col-6">
                <%= f.text_field :name,class:"input",placeholder:"Первоначальный взнос" %>
              </div>
              <div class="col-6">
                <%= f.text_field :name,class:"input",placeholder:"ФИО" %>
              </div>
              <div class="col-6">
                <%= f.text_field :name,class:"input",placeholder:"Регион регистрации" %>
              </div>
              <div class="col-6">
