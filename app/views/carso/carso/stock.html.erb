              <%= f.phone_field :phone,class:"input",data:{:role=>"phone"},placeholder:"+7 (___) ___-__-__" %>
            </div>
            <%= f.button class: "btn btn-blue" do %><span>Участвовать в акции</span><% end %>
          <% end %>
        </div>
        <div class="stocks_page_item__image lazy" data-bg="img/stocks_image_01.png"></div>
      </div>
      <div class="stocks_page_item">
        <div class="stocks_page_item__image lazy" data-bg="img/stocks_image_02.png"></div>
        <div class="stocks_page_item__content">
          <div class="stocks_page_item__content__title">Осенний ценопад:</div>
          <div class="stocks_page_item__content__subtitle">Скидки <br>до 40%</div>
          <%= form_for Ticket.new,url: {action:"create",controller:"tickets" },remote:true,validate:true,:html => {:class => "stocks_page_item__content__form",:name=>"newcarreq"},:namespace => "newcarreq" do |f| %>
            <%= hidden_field_tag 'ticket[other][model]', id:'newcarreq_ticket_other_model' %>
